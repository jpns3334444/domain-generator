{
  "name": "domain-generator-backend",
  "version": "1.0.0",
  "description": "AWS Lambda WHOIS lookup service",
  "main": "dist/handlers/whois.js",
  "scripts": {
    "build": "tsc",
    "package": "npm run build && cd dist && zip -r ../function.zip .",
    "deploy": "aws cloudformation deploy --template-file cloudformation.yaml --stack-name domain-generator-api --capabilities CAPABILITY_IAM",
    "seed-prompts": "npx ts-node scripts/seed-prompts.ts",
    "seed-prompts:clear": "npx ts-node scripts/seed-prompts.ts --clear"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.700.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  }
}
